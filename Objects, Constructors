
///1. Create a Student class with properties (name, age, grade) and a constructor
 static void Main(string[] args)
 {
     stuudent s1 = new stuudent("moin", 22,'A');
     stuudent s2 = new stuudent("mahesh", 23, 'A');
     Console.WriteLine(s1.name);
     Console.ReadLine();

 }
class stuudent
{
    public string name;
    public int age;
    public char grade;

    public stuudent(string name, int age, char grade)
    {
        this.name = name;
        this.age = age;
        this.grade = grade;
       
    }
}


